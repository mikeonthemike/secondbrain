# Obsidian Second Brain Configuration
# Copy this file to .env and fill in your values

# Obsidian vault path (optional - can be configured via setup script)
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault

# Email import settings (optional)
EMAIL_IMPORT_ENABLED=false
EMAIL_LABEL_TO_PROCESS=To Process

# Backup settings (optional)
AUTO_BACKUP_ENABLED=true
BACKUP_LOCATION=/path/to/backup/location

# Web scraping settings (optional)
WEB_SCRAPING_ENABLED=false
USER_AGENT=Obsidian-Second-Brain/1.0
